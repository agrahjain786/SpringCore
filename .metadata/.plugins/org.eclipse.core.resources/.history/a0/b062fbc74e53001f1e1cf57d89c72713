package com.techlabs.app.dto;

import java.util.Set;

import lombok.Data;
import lombok.NoArgsConstructor;


@Data
@NoArgsConstructor
public class StudentDTO {
	
	private Long id;
    private String name;
    private Set<CourseDTO> courses;
    
    
	public StudentDTO(Long id, String name) {
		super();
		this.id = id;
		this.name = name;
	}
    
    

}
